{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "directorist-gutenberg/listings-archive-filters",
	"version": "0.1.0",
	"title": "Listings Archive Filters",
	"description": "Displays the listings archive filters",
	"category": "directorist-listings-archive",
	"attributes": {
		"directory_type_id": {
			"description": "The ID of the directory type to display filters for",
			"type": "number",
			"default": 0
		},
		"filters_text": {
			"description": "The title of the filters block",
			"type": "string",
			"default": "Filters"
		},
		"reset_text": {
			"description": "The text of the reset button",
			"type": "string",
			"default": "Clear All"
		},
		"block_width": {
			"description": "Block width is used to set the width of the block in percentage",
			"type": "string",
			"default": "100"
		}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "directorist-gutenberg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
