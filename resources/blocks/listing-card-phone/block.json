{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "directorist-gutenberg/listing-card-phone",
	"version": "0.1.0",
	"title": "Listing Phone",
	"category": "directorist-listing-card-preset-fields",
	"attributes": {
		"meta_key": {
			"type": "string",
			"default": "phone"
		},
		"icon": {
			"type": "string",
			"default": "line-awesome/phone-solid.svg"
		},
		"show_label": {
			"type": "boolean",
			"default": false
		},
		"icon_color": {
			"type": "string",
			"default": ""
		},
		"icon_size": {
			"type": "string",
			"default": "16px"
		},
		"block_width": {
			"type": "string",
			"default": "100"
		},
		"textAlign": {
			"type": "string"
		}
	},
	"description": "Listing Phone Block",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"text": true,
			"background": true,
			"gradients": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true
		},
		"spacing": {
			"padding": true,
			"margin": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		}
	},
	"textdomain": "directorist-gutenberg",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
