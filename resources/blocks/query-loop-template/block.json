{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "directorist-gutenberg/query-loop-template",
  "version": "0.1.0",
  "title": "Query Loop Template",
  "description": "Loop through listings and display them with custom template",
  "category": "directorist-listings-archive",
  "parent": ["directorist-gutenberg/query"],
  "usesContext": [
    "directorist-gutenberg/queryId",
    "directorist-gutenberg/directoryTypeId",
    "directorist-gutenberg/perPage"
  ],
  "providesContext": {
    "postId": "postId",
    "postType": "postType"
  },
  "attributes": {
    "postId": {
      "type": "number"
    },
    "postType": {
      "type": "string",
      "default": "at_biz_dir"
    }
  },
  "supports": {
    "html": false,
    "reusable": false
  },
  "textdomain": "directorist-gutenberg",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
  "render": "file:./render.php"
}
